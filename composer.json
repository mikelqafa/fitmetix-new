{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "5.2.*",
    "anhskohbo/no-captcha": "^2.4",
    "anlutro/l4-settings": "^0.5.0",
    "barryvdh/laravel-debugbar": "^3.1",
    "cmgmyr/messenger": "^2.15",
    "cocur/slugify": "^3.0",
    "cviebrock/eloquent-sluggable": "^4.3",
    "ddctd143/google-translate": "^1.0",
    "dnoegel/php-xdg-base-dir": "^0.1.0",
    "erusev/parsedown": "^1.6",
    "ezyang/htmlpurifier": "^4.9",
    "fzaninotto/faker": "^1.7",
    "graham-campbell/manager": "^3.0",
    "hamcrest/hamcrest-php": "^2.0",
    "intervention/image": "^2.4",
    "jeremeamia/superclosure": "^2.3",
    "kasp3r/link-preview": "^2.0",
    "laracasts/flash": "^3.0",
    "laravelcollective/html": "^5.5",
    "maximebf/debugbar": "^1.14",
    "mews/purifier": "^2.0",
    "mtdowling/cron-expression": "^2.0",
    "myclabs/deep-copy": "^1.7",
    "nesbot/carbon": "^1.22",
    "nikic/php-parser": "^3.1",
    "paragonie/random_compat": "^2.0",
    "guzzlehttp/guzzle": "~5.2",
    "paypal/rest-api-sdk-php": "*",
    "predis/predis": "^1.1",
    "prettus/l5-repository": "^2.6",
    "pusher/pusher-php-server": "^3.0",
    "rachidlaasri/laravel-installer": "^3.0",
    "ramsey/uuid": "^3.7",
    "teepluss/theme": "^2.0",
    "tijsverkoyen/css-to-inline-styles": "^2.2",
    "twig/twig": "^2.4",
    "vijaytupakula/transvel": "^1.0",
    "vinkla/pusher": "^4.0",
    "zircote/swagger-php": "^2.0",
    "zizaco/entrust": "^1.8"
  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "symfony/css-selector": "2.8.*|3.0.*",
    "symfony/dom-crawler": "2.8.*|3.0.*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": ["app/Http/helpers.php"]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
